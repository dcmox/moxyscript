{
    "compilerOptions": {
      "target": "esnext",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext",
        "es2016"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "none",
      "noEmit": true
    },
    "include": [
      "./public/**/**/*",
      "./public/content/**/*",
      "*",
      "./lib/*"
    ],
    "exclude": [
      "node_modules"
    ]
}
  